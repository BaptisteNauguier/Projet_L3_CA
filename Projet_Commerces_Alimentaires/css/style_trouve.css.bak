html, body {
	margin: 0px auto;
}


main{
	margin-left: auto;
    margin-right: auto;
    max-width: 800px;
}

.d-flex{
	display: flex;
    flex-wrap: wrap;
    margin-right: -15px;
    margin-left: -15px;
}

.col {
    -ms-flex-preferred-size: 0;
    flex-basis: 0;
    -webkit-box-flex: 1;
    -ms-flex-positive: 1;
    flex-grow: 1;
    max-width: 100%;
}

.ml-3{
	margin-left: 10px;
}

.navbar{
  width: 100%;
  background-color: #ffff89; 
}

.navbar-ul {
  list-style-type: none;
    margin-top: 0px;
  margin-left: auto;
  margin-right: auto;
  width: 680px;
  overflow: hidden;
}

.nav-item {
  display: inline-block;
  border-right: 1px solid #ffcc50;
}

.nav-link {
  display: flex;
  color: #333;
  text-align: center;
  padding: 14px 30px;
  text-decoration: none;
  margin-left: -4Px;
}

.nav-link:hover {
  background-color:#ffcc50;
  color:white;
}

.active {
  background-color: #b65c02;
  color: #fff !important;
}

.text-center{
	text-align: center;
}

.titre{
	background-color: #333;
	padding: 10px;
	border-radius: 10px;
	color: white;
	margin-left: auto;
    width: 400px;
    margin-right: auto;
}

input[type=text] {
  font-size: 17px;
  border: none;
  background: #fff;
  width:90%;
  text-align:center;
}

input[type=text]:focus {
    border-color: none;
    outline: 0;
}

.search-container{
	width: 100%;
	background: #fff;
	border: 1px solid #ced4da;
	line-height: 1.5;	
	color: #495057;
	padding: 10px;
	border-radius: 10px;
	margin-bottom: 20px;
}

.search-container button {
  background: #fff;
  font-size: 17px;
  border: none;
  cursor: pointer;
}

.circleBase {
    border-radius: 50%;
	display: inline-block;
	margin: auto;
	min-width: 80px;
	transition: all .2s ease-in-out;
}


div[class*='type'] {
   padding:5px;
   cursor:pointer;
}


div[class*='type']:hover {
   transform: scale(1.1);
}

